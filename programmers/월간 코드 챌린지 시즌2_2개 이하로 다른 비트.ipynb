        "# 월간 코드 챌린지 시즌2 _ 2개 이하로 다른 비트\n",
        "def solution(numbers):\n",
        "    answer = []\n",
        "    for i in range(len(numbers)):\n",
        "        if numbers[i] % 2 == 0:\n",
        "            answer.append(numbers[i] + 1)\n",
        "        elif numbers[i] % 2 != 0:\n",
        "            k = bin(numbers[i])[2:]\n",
        "            if k[len(k)-2:] == '01':\n",
        "                answer.append(numbers[i] + 1)\n",
        "            else:\n",
        "                bn = list(bin(numbers[i])[2:])\n",
        "                ze = 0\n",
        "                for j in range(len(bn)-1,-1,-1):\n",
        "                    if bn[j] == '1':\n",
        "                        ze += 1\n",
        "                    else:\n",
        "                        break\n",
        "                answer.append(numbers[i] + (2**((ze)-1)))\n",
        "    return answer\n",
        "solution([2,7])"
      ],
